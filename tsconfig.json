{
  "extends": "expo/tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true, // <--- ADD THIS LINE
    "paths": {
      "@/*": [
        "./*"
      ]
    }
    // ... any other compiler options you have
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}